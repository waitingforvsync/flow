\\ !Boot file

ORG &1200

.boot
{
    CLI
    LDX #0
    .loop
    LDA cmd+1,X:JSR &FFE3
    INX
    CMP #13
    BNE loop
    JSR &FFE7

    LDA #19:JSR &FFF4
    LDA #15:LDX #0:JSR &FFF4
    LDX #LO(cmd)
    LDY #HI(cmd)
    JMP &FFF7

    .cmd
    EQUS "/FLOW", 13
    EQUS "V0.01", 13, "Rich Talbot-Watkins, 2025"
}
